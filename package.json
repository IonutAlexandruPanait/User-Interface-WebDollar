{
  "name": "@webdollar/user-interface-webdollar",
  "version": "0.0.1",
  "description": "User Interface for WebDollar",
  "main": "index.js",
  "scripts": {
    "open_browser": "node src/open-browser",
    "build_frontend": "browserify src/main.js -o dist_bundle/../../vue-Frontend/public/WebDollar-dist/WebDollar-User-Interface-bundle.js -t vueify -t [ stringify --extensions [ .html .css .text .svg ] ] -t [ babelify --presets [ es2017 ] ]   ",
    "build": "browserify src/main.js -o dist_bundle/WebDollar-User-Interface-bundle.js -t vueify -t [ stringify --extensions [ .html .css .text .svg ] ] -t [ babelify --presets [ es2017 ] ]   && npm run open_browser && npm run build_frontend",
    "test": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WebDollar/User-Interface-WebDollar.git"
  },
  "keywords": [
    "WebDollar",
    "User Interface",
    "UI",
    "UX",
    "User Experience"
  ],
  "author": "WebDollar",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/WebDollar/User-Interface-WebDollar/issues"
  },
  "homepage": "https://github.com/WebDollar/User-Interface-WebDollar#readme",
  "dependencies": {
    "brfs": "^1.4.3",
    "browserify-fs": "^1.0.0",
    "clipboard": "^1.7.1",
    "md5": "^2.2.1",
    "opn": "^5.1.0",
    "stringify": "^5.2.0",
    "vue": "^2.5.13",
    "vue-slider-component": "*"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-module-alias": "^1.6.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2017": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.5.0",
    "vue-hot-reload-loader": "^0.1.5",
    "vueify": "^9.4.1"
  }
}
